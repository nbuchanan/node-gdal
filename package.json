{
  "name": "gdal-mbt",
  "version": "0.9.4",
  "description": "Bindings to GDAL (Geospatial Data Abstraction Library) with sqlite3 and mbtiles drivers enabled.",
  "license": "Apache-2.0",
  "url": "https://github.com/nbuchanan/node-gdal/tree/gdal-2.2",
  "main": "./lib/gdal.js",
  "keywords": [
    "gdal",
    "libgdal",
    "gis",
    "geo",
    "geos",
    "geography",
    "geospatial",
    "raster",
    "vector",
    "gtiff",
    "geojson",
    "mapping",
    "sqlite",
    "mbtiles"
  ],
  "binary": {
    "module_name": "gdal-mbt",
    "module_path": "./lib/binding/{node_abi}-{platform}-{arch}",
    "remote_path": "./{module_name}/v{version}/{toolset}/",
    "host": "https://asaspades-builds.s3.amazonaws.com",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nbuchanan/node-gdal.git"
  },
  "authors": [
    {
      "name": "Brandon Reavis",
      "url": "https://github.com/brandonreavis",
      "email": "brandon@naturalatlas.com"
    },
    {
      "name": "Nick Buchanan",
      "url": "https://github.com/nbuchanan",
      "email": "nbuchanan@gmail.com"
    }
  ],
  "scripts": {
    "test": "mocha test -R tap --timeout 600000 --no-colors -gc --require ./test/_common.js",
    "test-syntax": "eslint lib test --fix",
    "preinstall":"npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
    "postpublish": "npm run publish-yuidoc",
    "yuidoc": "yuidoc --extension .js,.cpp,.hpp",
    "publish-yuidoc": "npm run yuidoc && node ./scripts/publish-docs.js"
  },
  "dependencies": {
    "nan": "~2.5.0",
    "node-pre-gyp": "~0.6.34"
  },
  "devDependencies": {
    "aws-sdk": "~2.0.25",
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "eslint-config-naturalatlas": "latest",
    "gh-pages": "~0.2.0",
    "mocha": "^2.3.4",
    "yuidoc-lucid-theme": "~0.1.1",
    "yuidocjs": "~0.3.50"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "engine-strict": true
}
